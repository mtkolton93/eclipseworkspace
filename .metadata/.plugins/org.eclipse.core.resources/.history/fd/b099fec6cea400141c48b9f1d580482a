package consoleBlackjack;

import 

public class Deck
{
	Card[] deck = new Card[52];
	
	public Deck()
	{
		for(int i = 0; i < 52; i++)
		{
			deck[i] = new Card(i);
		}
	}
	
	public void shuffle()
	{
		
	}
	
	public Card drawCard()
	{
		Card ret = deck[0];
		return ret;
	}
}

